<% provide(:title, 'New Subcategory') %>
<h1>Create New Subcategory</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@category, url: categories_path) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Add Subcategory", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
